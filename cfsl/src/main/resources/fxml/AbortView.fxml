<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.shape.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Group fx:id="root" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Pane fx:id="headWrapper" layoutX="145.0" layoutY="-10.0" prefHeight="20.0" prefWidth="10.0" rotate="70.5">
         <children>
            <Polygon layoutX="5.0" layoutY="10.0" stroke="BLACK" strokeType="INSIDE" styleClass="edge-head">
              <points>
                <Double fx:value="0.0" />
                <Double fx:value="0.0" />
                
                <Double fx:value="5.0" />
                <Double fx:value="10.0" />
                
                <Double fx:value="-5.0" />
                <Double fx:value="10.0" />
              </points>
            </Polygon>
         </children>
      </Pane>
      <QuadCurve fx:id="curve" controlX="75.0" controlY="25.0" endX="150.0" fill="#ffffff00" startY="50.0" stroke="BLACK" strokeWidth="2.0" styleClass="edge-curve" />
      <VBox fx:id="textInputContainer" alignment="CENTER">
         <children>
            <Label fx:id="label" text="Label" />
         </children>
      </VBox>
   </children>
   <styleClass>
      <String fx:value="edge" />
      <String fx:value="abort" />
   </styleClass>
</Group>
